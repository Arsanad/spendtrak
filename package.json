{
  "name": "spendtrak",
  "version": "2.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "expo start --dev-client",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "prebuild:production": "node scripts/verify-no-dev-auth.js",
    "build:production": "npm run prebuild:production && eas build --profile production",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:db:push": "npx supabase db push",
    "supabase:db:reset": "npx supabase db reset",
    "supabase:functions:serve": "npx supabase functions serve",
    "supabase:functions:deploy": "npx supabase functions deploy",
    "lint": "eslint src/ app/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ app/ --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:services": "jest --testPathPattern=services",
    "test:components": "jest --testPathPattern=components",
    "test:stores": "jest --testPathPattern=stores",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "e2e": "maestro test maestro/run-all-tests.yaml",
    "e2e:auth": "maestro test maestro/tests/01-authentication.yaml",
    "e2e:dashboard": "maestro test maestro/tests/02-dashboard.yaml",
    "e2e:transactions": "maestro test maestro/tests/03-transactions.yaml",
    "e2e:add-expense": "maestro test maestro/tests/04-add-expense.yaml",
    "e2e:receipt": "maestro test maestro/tests/05-receipt-scanner.yaml",
    "e2e:subscriptions": "maestro test maestro/tests/06-subscriptions.yaml",
    "e2e:alerts": "maestro test maestro/tests/07-alerts.yaml",
    "e2e:ai": "maestro test maestro/tests/08-ai-consultant.yaml",
    "e2e:settings": "maestro test maestro/tests/09-settings.yaml",
    "e2e:budgets": "maestro test maestro/tests/10-budgets.yaml",
    "e2e:goals": "maestro test maestro/tests/11-goals.yaml",
    "e2e:cards": "maestro test maestro/tests/12-cards.yaml",
    "e2e:transaction-detail": "maestro test maestro/tests/13-transaction-detail.yaml",
    "e2e:navigation": "maestro test maestro/tests/14-navigation.yaml",
    "e2e:theme": "maestro test maestro/tests/15-theme.yaml",
    "e2e:full": "maestro test maestro/run-all-tests.yaml",
    "test:all": "npm test && npm run e2e:full",
    "prepare-icon": "node scripts/prepare-icon.js"
  },
  "dependencies": {
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@expo-google-fonts/cinzel": "^0.4.2",
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/native": "^7.1.8",
    "@sentry/react-native": "~7.2.0",
    "@supabase/supabase-js": "^2.90.1",
    "date-fns": "^4.1.0",
    "expo": "~54.0.33",
    "expo-apple-authentication": "~8.0.8",
    "expo-asset": "~12.0.12",
    "expo-auth-session": "~7.0.10",
    "expo-blur": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-gl": "~16.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "^31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-three": "^8.0.0",
    "expo-updates": "~29.0.16",
    "expo-video": "~3.0.15",
    "expo-web-browser": "~15.0.10",
    "jail-monkey": "^2.8.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-purchases": "^9.7.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "three": "^0.166.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.75.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/detox": "^17.14.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "@types/three": "^0.166.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.9",
    "detox": "^20.46.3",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.0.0",
    "jest": "~29.7.0",
    "jest-circus": "~29.7.0",
    "jest-expo": "~54.0.17",
    "jsonwebtoken": "^9.0.3",
    "lint-staged": "^15.0.0",
    "prettier": "^3.4.0",
    "react-test-renderer": "^19.1.0",
    "sharp": "^0.34.5",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
